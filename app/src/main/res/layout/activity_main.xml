<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="comdeep.example.classroom.vsp.MainActivity"
   android:background="@drawable/bg_login" >


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="160dp"
        android:layout_height="160dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="70dp"
        android:layout_marginTop="68dp"
        app:srcCompat="@drawable/login_icon" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView4"
        android:layout_marginStart="22dp"
        android:layout_toEndOf="@+id/imageView4"
        app:srcCompat="@drawable/q" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:layout_alignTop="@+id/imageView6"
        android:layout_marginStart="45dp"
        android:layout_toEndOf="@+id/imageView"
        app:srcCompat="@drawable/empty_profile"
        android:onClick="openPerson"
        />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/imageView4"
        android:layout_marginBottom="23dp"
        app:srcCompat="@drawable/inaugration"
        android:onClick="openInauguration"/>

</RelativeLayout>
